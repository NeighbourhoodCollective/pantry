{
  "name": "@opensaas-clubhouse/frontend",
  "version": "1.0.0",
  "private": true,
  "description": "Clubhouse Portal Frontend",
  "repository": "https://github.com/OpenSaasAU/clubhouse/tree/master/apps/user-portal",
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "scripts": {
    "build": "ts-gql build && next build",
    "dev": "next -p 7777",
    "postinstall": "ts-gql build",
    "start": "node server.js",
    "start:next": "next start",
    "test": "NODE_ENV=test jest --watch",
    "ts-gql:build": "ts-gql build",
    "ts-gql:watch": "ts-gql watch"
  },
  "eslintConfig": {
    "extends": "next/core-web-vitals",
    "rules": {
      "@typescript-eslint/no-unsafe-assignment": 0,
      "react/function-component-definition": [
        2,
        {
          "namedComponents": "function-declaration",
          "unnamedComponents": "function-expression"
        }
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx",
            "tsx"
          ]
        }
      ],
      "react/jsx-props-no-spreading": [
        1,
        {
          "exceptions": [
            "Component",
            "App"
          ]
        }
      ]
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@apollo/server": "^4.4.0",
    "@as-integrations/next": "^1.2.0",
    "@aws-sdk/abort-controller": "3.267.0",
    "@aws-sdk/signature-v4-crt": "3.267.0",
    "@babel/core": "^7.18.5",
    "@emotion/css": "^11.10.0",
    "@emotion/react": "^11.10.0",
    "@keystone-6/document-renderer": "^1.1.1",
    "@keystone-ui/button": "^7.0.1",
    "@keystone-ui/fields": "^7.1.1",
    "@keystone-ui/icons": "^6.0.1",
    "@next/bundle-analyzer": "^13.2.3",
    "@opensaas-clubhouse/backend": "workspace:*",
    "@prisma/client": "4.11.0",
    "@prisma/nextjs-monorepo-workaround-plugin": "^4.11.0",
    "@stripe/react-stripe-js": "^1.10.0",
    "@stripe/stripe-js": "^1.47.0",
    "@ts-gql/compiler": "^0.16.3",
    "@ts-gql/eslint-plugin": "^0.8.5",
    "@ts-gql/next": "^17.0.0",
    "@ts-gql/tag": "^0.7.0",
    "apollo-upload-client": "^17.0.0",
    "aws-crt": "1.15.9",
    "core-js": "^3.25.0",
    "date-fns": "^2.29.2",
    "deepmerge": "^4.2.2",
    "downshift": "^6.1.9",
    "encoding": "^0.1.13",
    "eslint-config-next": "^13.2.3",
    "graphql": "^16.6.0",
    "graphql-upload": "^16.0.2",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "next": "^13.2.3",
    "next-auth": "^4.20.0",
    "next-with-apollo": "^5.3.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-player": "^2.10.1",
    "react-transition-group": "^4.4.5",
    "styled-components": "^5.3.5",
    "typescript": "^4.8.2",
    "waait": "^1.0.5"
  },
  "devDependencies": {
    "@apollo/react-testing": "^4.0.0",
    "@babel/eslint-parser": "^7.18.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^13.5.0",
    "@types/apollo-upload-client": "^17.0.0",
    "@types/lodash": "^4.14.184",
    "@types/node": "18.14.4",
    "@types/nprogress": "^0.2.0",
    "@types/react": "18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "babel-jest": "^27.5.1",
    "babel-plugin-styled-components": "^2.0.7",
    "casual": "^1.6.2",
    "eslint": "8.35.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^27.5.1",
    "prettier": "^2.7.1",
    "prisma": "^4.11.0",
    "react-test-renderer": "^18.2.0"
  },
  "packageManager": "pnpm@7.33.7",
  "ts-gql": {
    "mode": "no-transform",
    "schema": "../../backend/schema.graphql"
  }
}