{
  "name": "@opensaas-clubhouse/frontend",
  "version": "1.0.0",
  "private": true,
  "description": "Clubhouse Portal Frontend",
  "repository": "https://github.com/OpenSaasAU/clubhouse/tree/master/apps/user-portal",
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "scripts": {
    "build": "ts-gql build && next build",
    "dev": "next -p 7777",
    "postinstall": "ts-gql build",
    "start": "node server.js",
    "test": "NODE_ENV=test jest --watch",
    "ts-gql:build": "ts-gql build",
    "ts-gql:watch": "ts-gql watch"
  },
  "eslintConfig": {
    "extends": [
      "wesbos",
      "next"
    ],
    "rules": {
      "@typescript-eslint/no-unsafe-assignment": 0,
      "react/function-component-definition": [
        2,
        {
          "namedComponents": "function-declaration",
          "unnamedComponents": "function-expression"
        }
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx",
            "tsx"
          ]
        }
      ],
      "react/jsx-props-no-spreading": [
        1,
        {
          "exceptions": [
            "Component",
            "App"
          ]
        }
      ]
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@babel/core": "^7.18.5",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/runtime": "^7.18.3",
    "@babel/runtime-corejs3": "^7.18.3",
    "@emotion/css": "^11.10.0",
    "@emotion/react": "^11.10.0",
    "@keystone-6/document-renderer": "^1.1.1",
    "@keystone-ui/button": "^7.0.1",
    "@keystone-ui/fields": "^7.1.1",
    "@keystone-ui/icons": "^6.0.1",
    "@next/bundle-analyzer": "^12.2.5",
    "@preconstruct/next": "^4.0.0",
    "@stripe/react-stripe-js": "^1.10.0",
    "@stripe/stripe-js": "^1.35.0",
    "@ts-gql/compiler": "^0.15.3",
    "@ts-gql/eslint-plugin": "^0.8.3",
    "@ts-gql/next": "^16.0.3",
    "@ts-gql/tag": "^0.7.0",
    "apollo-upload-client": "^17.0.0",
    "core-js": "^3.25.0",
    "date-fns": "^2.29.2",
    "deepmerge": "^4.2.2",
    "downshift": "^6.1.9",
    "eslint-config-next": "^12.2.5",
    "graphql": "^15.8.0",
    "graphql-upload": "^16.0.2",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "next": "^12.2.5",
    "next-auth": "^4.18.8",
    "next-transpile-modules": "^9.0.0",
    "next-with-apollo": "^5.3.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-player": "^2.10.1",
    "react-transition-group": "^4.4.5",
    "styled-components": "^5.3.5",
    "typescript": "^4.8.2",
    "waait": "^1.0.5"
  },
  "devDependencies": {
    "@apollo/react-testing": "^4.0.0",
    "@babel/eslint-parser": "^7.18.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^13.5.0",
    "@types/apollo-upload-client": "^17.0.0",
    "@types/lodash": "^4.14.184",
    "@types/nprogress": "^0.2.0",
    "@types/react": "18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "babel-jest": "^27.5.1",
    "babel-plugin-styled-components": "^2.0.7",
    "casual": "^1.6.2",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-wesbos": "^3.0.2",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^27.5.1",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0"
  },
  "packageManager": "yarn@1.22.19",
  "ts-gql": {
    "mode": "no-transform",
    "schema": "../../backend/schema.graphql"
  }
}